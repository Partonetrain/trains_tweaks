{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://fabricmc.net/",
        "sources": "https://github.com/Partonetrain/trains_tweaks"
    },
    "license": "${license}",
    "icon": "assets/trains_tweaks/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "info.partonetrain.trains_tweaks.TrainsTweaksFabric"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=${java_version}",
        "forgeconfigapiport": ">=21.1.2"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  